#include<iostream>

using namespace std;

int main() {
	int n, m;
	int answer = 0;
	cin >> n >> m;
	if (m%n != 0 || m < n)		// m이 n의 배수가 아닌 경우
		answer = -1;
	else if (m == n)			// m과 n이 같은 경우
		answer = 0;
	else if(m%n == 0){			// 가능성이 있는 경우
		int quo = m / n;
		while (quo % 3 == 0) {	// 3으로 계속 나눠준다
			quo /= 3;
			answer++;
		}
		while (quo % 2 == 0) {	// 2로 계속 나눠준다
			quo /= 2;
			answer++;
		}
		if (quo != 1)			// 배수지만 2와 3이외의 소인수를 갖는 경우
			answer = -1;
	}
	cout << answer;
	return 0;
}
